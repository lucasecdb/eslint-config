{
  "name": "@lucasecdb/eslint-config",
  "version": "0.14.0",
  "description": "My personal base config for eslint",
  "main": "index.js",
  "repository": "https://github.com/lucasecdb/eslint-config.git",
  "scripts": {
    "eslint-check": "eslint --print-config index.js | eslint-config-prettier-check",
    "lint": "eslint ."
  },
  "author": "Lucas Cordeiro",
  "license": "MIT",
  "files": [
    "index.js",
    "rules/"
  ],
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "~7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "peerDependencies": {
    "eslint": "^7.5.0",
    "prettier": "^2.0.1",
    "typescript": "^3.3.3333 || ^4.0.0"
  },
  "devDependencies": {
    "eslint": "^7.12.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
